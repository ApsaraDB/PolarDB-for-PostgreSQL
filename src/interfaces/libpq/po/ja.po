msgid "unexpected shape of result set returned for SHOW\n"
msgstr "SHOW に対する予期しない形のリザルトセット\n"

#: fe-auth.c:1235
msgid "password_encryption value too long\n"
msgstr "password_encryptionの値が長すぎます\n"

