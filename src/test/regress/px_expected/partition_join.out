         ->  Append
               ->  Seq Scan on prt2_p2
                     Filter: (b > 250)
               ->  Seq Scan on prt2_p3
                     Filter: (b > 250)
         ->  Hash
               ->  Append
                     ->  Seq Scan on prt1_p1
                           Filter: ((a < 450) AND (b = 0))
                     ->  Seq Scan on prt1_p2
                           Filter: ((a < 450) AND (b = 0))
(15 rows)

