--
-- Test for pg_get_object_address
--
-- Clean up in case a prior regression run failed
SET client_min_messages TO 'warning';
DROP ROLE IF EXISTS regress_addr_user;
ERROR:  cannot execute DROP ROLE in a read-only transaction
RESET client_min_messages;
CREATE USER regress_addr_user;
ERROR:  cannot execute CREATE ROLE in a read-only transaction
-- Test generic object addressing/identification functions
CREATE SCHEMA addr_nsp;
ERROR:  cannot execute CREATE SCHEMA in a read-only transaction
SET search_path TO 'addr_nsp';
CREATE FOREIGN DATA WRAPPER addr_fdw;
ERROR:  cannot execute CREATE FOREIGN DATA WRAPPER in a read-only transaction
CREATE SERVER addr_fserv FOREIGN DATA WRAPPER addr_fdw;
ERROR:  cannot execute CREATE SERVER in a read-only transaction
CREATE TEXT SEARCH DICTIONARY addr_ts_dict (template=simple);
ERROR:  cannot execute CREATE TEXT SEARCH DICTIONARY in a read-only transaction
CREATE TEXT SEARCH CONFIGURATION addr_ts_conf (copy=english);
ERROR:  cannot execute CREATE TEXT SEARCH CONFIGURATION in a read-only transaction
CREATE TEXT SEARCH TEMPLATE addr_ts_temp (lexize=dsimple_lexize);
ERROR:  cannot execute CREATE TEXT SEARCH TEMPLATE in a read-only transaction
CREATE TEXT SEARCH PARSER addr_ts_prs
    (start = prsd_start, gettoken = prsd_nexttoken, end = prsd_end, lextypes = prsd_lextype);
ERROR:  cannot execute CREATE TEXT SEARCH PARSER in a read-only transaction
CREATE TABLE addr_nsp.gentable (
	a serial primary key CONSTRAINT a_chk CHECK (a > 0),
	b text DEFAULT 'hello');
ERROR:  cannot execute CREATE TABLE in a read-only transaction
CREATE TABLE addr_nsp.parttable (
	a int PRIMARY KEY
) PARTITION BY RANGE (a);
ERROR:  cannot execute CREATE TABLE in a read-only transaction
CREATE VIEW addr_nsp.genview AS SELECT * from addr_nsp.gentable;
ERROR:  cannot execute CREATE VIEW in a read-only transaction
CREATE MATERIALIZED VIEW addr_nsp.genmatview AS SELECT * FROM addr_nsp.gentable;
ERROR:  cannot execute CREATE MATERIALIZED VIEW in a read-only transaction
CREATE TYPE addr_nsp.gencomptype AS (a int);
ERROR:  cannot execute CREATE TYPE in a read-only transaction
CREATE TYPE addr_nsp.genenum AS ENUM ('one', 'two');
ERROR:  cannot execute CREATE TYPE in a read-only transaction
CREATE FOREIGN TABLE addr_nsp.genftable (a int) SERVER addr_fserv;
ERROR:  cannot execute CREATE FOREIGN TABLE in a read-only transaction
CREATE AGGREGATE addr_nsp.genaggr(int4) (sfunc = int4pl, stype = int4);
ERROR:  cannot execute CREATE AGGREGATE in a read-only transaction
CREATE DOMAIN addr_nsp.gendomain AS int4 CONSTRAINT domconstr CHECK (value > 0);
ERROR:  cannot execute CREATE DOMAIN in a read-only transaction
CREATE FUNCTION addr_nsp.trig() RETURNS TRIGGER LANGUAGE plpgsql AS $$ BEGIN END; $$;
ERROR:  cannot execute CREATE FUNCTION in a read-only transaction
CREATE TRIGGER t BEFORE INSERT ON addr_nsp.gentable FOR EACH ROW EXECUTE PROCEDURE addr_nsp.trig();
ERROR:  cannot execute CREATE TRIGGER in a read-only transaction
CREATE POLICY genpol ON addr_nsp.gentable;
ERROR:  cannot acquire lock mode AccessExclusiveLock on database objects while recovery is in progress
HINT:  Only RowExclusiveLock or less can be acquired on database objects during recovery.
CREATE PROCEDURE addr_nsp.proc(int4) LANGUAGE SQL AS $$ $$;
ERROR:  cannot execute CREATE PROCEDURE in a read-only transaction
CREATE SERVER "integer" FOREIGN DATA WRAPPER addr_fdw;
ERROR:  cannot execute CREATE SERVER in a read-only transaction
CREATE USER MAPPING FOR regress_addr_user SERVER "integer";
ERROR:  cannot execute CREATE USER MAPPING in a read-only transaction
ALTER DEFAULT PRIVILEGES FOR ROLE regress_addr_user IN SCHEMA public GRANT ALL ON TABLES TO regress_addr_user;
ERROR:  cannot execute ALTER DEFAULT PRIVILEGES in a read-only transaction
ALTER DEFAULT PRIVILEGES FOR ROLE regress_addr_user REVOKE DELETE ON TABLES FROM regress_addr_user;
ERROR:  cannot execute ALTER DEFAULT PRIVILEGES in a read-only transaction
-- this transform would be quite unsafe to leave lying around,
-- except that the SQL language pays no attention to transforms:
CREATE TRANSFORM FOR int LANGUAGE SQL (
	FROM SQL WITH FUNCTION prsd_lextype(internal),
	TO SQL WITH FUNCTION int4recv(internal));
ERROR:  cannot execute CREATE TRANSFORM in a read-only transaction
CREATE PUBLICATION addr_pub FOR TABLE addr_nsp.gentable;
ERROR:  cannot execute CREATE PUBLICATION in a read-only transaction
CREATE SUBSCRIPTION addr_sub CONNECTION '' PUBLICATION bar WITH (connect = false, slot_name = NONE);
ERROR:  cannot execute CREATE SUBSCRIPTION in a read-only transaction
CREATE STATISTICS addr_nsp.gentable_stat ON a, b FROM addr_nsp.gentable;
ERROR:  cannot acquire lock mode ShareUpdateExclusiveLock on database objects while recovery is in progress
HINT:  Only RowExclusiveLock or less can be acquired on database objects during recovery.
-- test some error cases
SELECT pg_get_object_address('stone', '{}', '{}');
ERROR:  unrecognized object type "stone"
SELECT pg_get_object_address('table', '{}', '{}');
ERROR:  name list length must be at least 1
SELECT pg_get_object_address('table', '{NULL}', '{}');
ERROR:  name or argument lists may not contain nulls
-- unrecognized object types
DO $$
DECLARE
	objtype text;
BEGIN
	FOR objtype IN VALUES ('toast table'), ('index column'), ('sequence column'),
		('toast table column'), ('view column'), ('materialized view column')
	LOOP
		BEGIN
			PERFORM pg_get_object_address(objtype, '{one}', '{}');
		EXCEPTION WHEN invalid_parameter_value THEN
			RAISE WARNING 'error for %: %', objtype, sqlerrm;
		END;
	END LOOP;
END;
$$;
