#: main/main.c:326
#, c-format
msgid "Options:\n"
msgstr "Параметри:\n"

#: main/main.c:327
#, c-format
msgid "  -B NBUFFERS        number of shared buffers\n"
msgstr "  -B NBUFFERS        число спільних буферів\n"

#: main/main.c:328
#, c-format
msgid "  -c NAME=VALUE      set run-time parameter\n"
