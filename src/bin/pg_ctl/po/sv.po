#: pg_ctl.c:1976
#, c-format
msgid "Usage:\n"
msgstr "Användning:\n"

#: pg_ctl.c:1977
#, c-format
msgid "  %s init[db]   [-D DATADIR] [-s] [-o OPTIONS]\n"
msgstr "  %s init[db]   [-D DATAKAT] [-s] [-o FLAGGOR]\n"

#: pg_ctl.c:1978
#, c-format
msgid ""
"  %s start      [-D DATADIR] [-l FILENAME] [-W] [-t SECS] [-s]\n"
"                    [-o OPTIONS] [-p PATH] [-c]\n"
msgstr ""
"  %s start      [-D DATAKAT] [-l FILNAMN] [-W] [-t SEK] [-s]\n"
"                    [-o FLAGGOR] [-p SOKVÄG] [-c]\n"

#: pg_ctl.c:1980
#, c-format
msgid "  %s stop       [-D DATADIR] [-m SHUTDOWN-MODE] [-W] [-t SECS] [-s]\n"
msgstr "  %s stop       [-D DATAKAT] [-m STÄNGNINGSMETOD] [-W] [-t SEK] [-s]\n"

#: pg_ctl.c:1981
#, c-format
msgid ""
"  %s restart    [-D DATADIR] [-m SHUTDOWN-MODE] [-W] [-t SECS] [-s]\n"
"                    [-o OPTIONS] [-c]\n"
msgstr ""
"  %s restart    [-D DATAKAT] [-m STÄNGNINGSMETOD] [-W] [-t SEK] [-s]\n"
"                    [-o FLAGGOR] [-c]\n"

#: pg_ctl.c:1983
#, c-format
msgid "  %s reload     [-D DATADIR] [-s]\n"
msgstr "  %s reload     [-D DATAKAT] [-s]\n"

#: pg_ctl.c:1984
#, c-format
msgid "  %s status     [-D DATADIR]\n"
msgstr "  %s status     [-D DATAKAT]\n"

#: pg_ctl.c:1985
#, c-format
msgid "  %s promote    [-D DATADIR] [-W] [-t SECS] [-s]\n"
msgstr "  %s promote    [-D DATAKAT] [-W] [-t SEK] [-s]\n"

#: pg_ctl.c:1986
#, c-format
msgid "  %s logrotate  [-D DATADIR] [-s]\n"
msgstr "  %s logrotate  [-D DATAKAT] [-s]\n"

#: pg_ctl.c:1987
#, c-format
msgid "  %s kill       SIGNALNAME PID\n"
msgstr "  %s kill       SIGNALNAMN PID\n"

