-- ----------------------------------------------------------------
-- Scalar Operations with Out-Of-Band Values.
-- NULL, UNDEFINED, EMPTY, EXPLICIT, SPARSE
-- (Sparse and compressed are the same internally)
-- ----------------------------------------------------------------
set extra_float_digits=0;
SELECT hll_set_output_version(1);
